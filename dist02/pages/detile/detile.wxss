}

.new_size_down {
    line-height: 50rpx;
    margin-top: 20rpx;
    color: #404040;
    font-size: 30rpx;
}

.new_size_top_border {
    font-size: 20rpx;
    padding-left: 5rpx;
    padding-right: 5rpx;
    color: red;
    line-height: 30rpx;
    border: 1rpx solid red;
}

.news_time_img {
    float: left;
    margin-top: 13rpx;
    height: 30rpx;
    width: 30rpx;
}

.bt_border {
