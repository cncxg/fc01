}

.message_modle_last {
    border-bottom: 1rpx solid white;
}

.news {
    height: 80rpx;
    width: 100%;
    border-top: 10rpx solid #F0F0F0;
    border-bottom: 10rpx solid #F0F0F0;
}

.news_title {
    width: 90%;
    color: #1d2a3d;
    font-size: 30rpx;
    margin-top: 20rpx;
    margin-left: 5%;
}

.news_img {
    position: absolute;
    height: 50rpx;
    width: 50rpx;
    right: 1%;
    margin-top: -50rpx;
}

.news_border {
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    width: 100%;
}

.news_border_text {
    float: left;
    width: 90%;
