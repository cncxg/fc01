.scroll {
    height: 500rpx;
    width: 100%;
}

.product_title {
    margin-top: 35rpx;
    width: 90%;
    color: #112240;
    margin-left: 5%;
    font-size: 50rpx;
}

.product_title_top {
    margin-top: 1rpx;
}

.data {
    font-size: 30rpx;
    color: #909090;
}

.price {
