    color: #e28043;
}

.message_modle {
    width: 94%;
    margin-left: 3%;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #F0F0F0;
}

.message_modle_text {
    line-height: 50rpx;
