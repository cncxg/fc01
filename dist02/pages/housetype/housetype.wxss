.type_border {
    height: 50rpx;
    padding-top: 15rpx;
    padding-bottom: 10rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    float: left;
    margin-left: 20rpx;
    margin-top: 20rpx;
    background: #F0F0F0;
    color: #666666;
}

.modle {
    height: 550rpx;
    width: 44%;
    margin-left: 4%;
    margin-top: 30rpx;
    float: left;
}

.modle_img {
    height: 330rpx;
    width: 100%;
    float: left;
    background: #f9f9f9;
}

.modle_title {
    float: left;
    font-size: 30rpx;
    width: 90%;
    margin-left: 5%;
    margin-top: 20rpx;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.active {
    color: white;
    background: #ef6c02;
}

.nav {
    height: 50rpx;
    width: 100%;
    float: left;
}

.banner {
    height: 500rpx;
    width: 100%;
    float: left;
    background: #f9f9f9;
}

.daishou_modle {
    height: 100rpx;
    width: 100%;
    text-align: center;
    line-height: 95rpx;
    float: left;
    font-size: 40rpx;
    color: white;
    background: #2881bb;
}

.housetype_border {
    padding-bottom: 20rpx;
    width: 100%;
    float: left;
    background: #d8eaf6;
}

.housetype_border_modle {
    height: 250rpx;
    width: 90%;
    margin-left: 5%;
    margin-top: 20rpx;
    background: #ffffff;
    border-radius: 20rpx;
    border: 1rpx solid #d4d9dc;
}

.housetype_border_modle_top {
    height: 100rpx;
    width: 100%;
    border-bottom: 1rpx solid #d4d9dc;
}

.housetype_border_modle_top_louceng {
    font-size: 45rpx;
    margin-top: 20rpx;
    float: left;
    color: #cf782b;
    margin-left: 3%;
}

.housetype_border_modle_top_time {
    font-size: 23rpx;
    margin-top: 40rpx;
    float: left;
    color: #666666;
    width: 35%;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-left: 2%;
}

.housetype_border_modle_down_message {
    width: 48%;
    float: left;
    color: #666666;
    margin-left: 2%;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-top: 20rpx;
    font-size: 27rpx;
}

.look_more_message {
    height: 80rpx;
    width: 90%;
    padding-left: 5%;
    padding-right: 5%;
    line-height: 75rpx;
    font-size: 30rpx;
    color: #666666;
    float: left;
    border-bottom: 10rpx solid #F0F0F0;
}

.look_more_message_img {
    height: 40rpx;
    width: 40rpx;
    float: right;
    margin-top: 20rpx;
}

.all_housetype {
    height: 80rpx;
    width: 90%;
    padding-left: 5%;
    padding-right: 5%;
    line-height: 75rpx;
    font-size: 30rpx;
    color: #666666;
    float: left;
    border-bottom: 5rpx solid #F0F0F0;
}

.daishou_title {
    position: absolute;
    float: right;
    margin-left: 283rpx;
    height: 30rpx;
    width: 50rpx;
    font-size: 20rpx;
    color: white;
    line-height: 33rpx;
    background: #237eb7;
    text-align: center;
}

.null_house {
    font-size: 30rpx;
    color: #666666;
    text-align: center;
    width: 100%;
    float: left;
    padding-bottom: 100rpx;
    height: 275px;
}